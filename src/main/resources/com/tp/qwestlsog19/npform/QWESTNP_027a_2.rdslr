rule "QWESTNP_027a_2" dialect "mvel" ruleflow-group "NPFORM"

when
		xcb object
 	 Evaluate 
Start Brace
 the field_tag "/LSR_ORD_REQ/NP" is exists
		 
AND
 Rule "QWESTNP_027a_2" multiple times "TRANSFER_OF_CALLS_GRP/TCID" in "/LSR_ORD_REQ/NP/NP_SVC_DET[i]" is populated
        
AND
 Rule "QWESTNP_027a_2" multiple times "TRANSFER_OF_CALLS_GRP/TCID" in "/LSR_ORD_REQ/NP/NP_SVC_DET[i]" is not equal to "4,5,6,25,30,39,45,55,65,70,71,74,75,76"
