rule "QWESTLSNP_017_1" dialect "mvel" ruleflow-group "LSNPFORM"

when
		xcb object
 	 Evaluate 
Start Brace
 the field_tag "/LSR_ORD_REQ/LSNP" is exists
		 
AND
 Rule "QWESTLSNP_017_1" multiple times "TDT" in "/LSR_ORD_REQ/LSNP/LSNP_SVC_DET[i]" is_not populated
        
