rule "QWESTDL_011_14" dialect "mvel" ruleflow-group "DLFORM"

when
		xcb object
 	 Evaluate 
Start Brace
 the field_tag "/LSR_ORD_REQ/DL" is exists
		 
AND
 Rule "QWESTDL_011_14" multiple times "LISTING_CNTRL/RTY" in "/LSR_ORD_REQ/DL/LISTING_INFO[i]" is populated
        
AND
 Rule "QWESTDL_011_14" multiple times "LISTING_CNTRL/RTY" in "/LSR_ORD_REQ/DL/LISTING_INFO[i]" is equal to "LCR,FCR"
        
AND
 Rule "QWESTDL_011_14" multiple times "LISTING_INSTRUCTION/LIST_TEXT_GRP/LTXTY" in "/LSR_ORD_REQ/DL/LISTING_INFO[i]" is equal to "CR"
		
AND
 Rule "QWESTDL_011_14" multiple times_the "1" position of "LISTING_INSTRUCTION/LIST_TEXT_GRP[i]/LTEXT" in "/LSR_ORD_REQ/DL/LISTING_INFO[i]" is equal to values "C"
		
AND
 Rule "QWESTDL_011_14" multiple times_the "2" position of "LISTING_INSTRUCTION/LIST_TEXT_GRP[i]/LTEXT" in "/LSR_ORD_REQ/DL/LISTING_INFO[i]" is equal to values "a"
		
AND
 Rule "QWESTDL_011_14" multiple times_the "3" position of "LISTING_INSTRUCTION/LIST_TEXT_GRP[i]/LTEXT" in "/LSR_ORD_REQ/DL/LISTING_INFO[i]" is equal to values "l"
		
AND
 Rule "QWESTDL_011_14" multiple times_the "4" position of "LISTING_INSTRUCTION/LIST_TEXT_GRP[i]/LTEXT" in "/LSR_ORD_REQ/DL/LISTING_INFO[i]" is equal to values "l"
		
AND
 Rule "QWESTDL_011_14" multiple times "LISTING_INSTRUCTION/LIST_ADDR_GRP/ADI" in "/LSR_ORD_REQ/DL/LISTING_INFO[i]" is populated
        
AND
 Rule "QWESTDL_011_14" multiple times "LISTING_INSTRUCTION/LIST_ADDR_GRP/ADI" in "/LSR_ORD_REQ/DL/LISTING_INFO[i]" is not equal to "O"
End Brace

 	 then
 	 	 the rule "QWESTDL_011_14" with error code "QWESTDL366" with message "RTY VALUE OF LCR, FCR REQUIRES ADI OF O WHEN LTXTY IS CR AND FIRST 4 CHARACTERS OF LTEXT ARE Call" for the field "listing_cntrl/rty" of section "/lsr_ord_req/dl/listing_info[i]"
end